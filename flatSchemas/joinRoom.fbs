namespace flats;

table RpPlayerSpawn {
    pid:ubyte; // 当局分配给玩家的id
    spawnAtX:short; // 出生点
    spawnAtY:short;
    health:ubyte; // 初始血量
    shield:ubyte; // 初始护甲值
}

table RpAllPlayerSpawns {
    pid:ubyte;
    others:[RpPlayerSpawn];
}

table RpMatchMaking {
    userId:uint;
    isJoin:byte; // 1: 加入, 0: 离开
}

table RpStartMatchMaking {
    battleServerIp:string;
    serverPort:ushort;
}

table RpPlayerSceneData {
    // 场景数据
}